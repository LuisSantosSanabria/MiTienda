{
  "version": "3.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "font-awesome@6.7.2",
      "destination": "wwwroot/lib/font-awesome/"
    },
    {
      "library": "sweetalert2@11.22.2",
      "destination": "wwwroot/lib/sweetalert2/"
    },
    {
      "provider": "jsdelivr",
      "library": "summernote@0.9.1",
      "destination": "wwwroot/lib/summernote/",
      "files": [
        "dist/summernote-bs5.min.css",
        "dist/summernote-bs5.min.js",
        "dist/summernote.min.css",
        "dist/summernote.min.js",
        "dist/font/summernote.eot",
        "dist/font/summernote.hash",
        "dist/font/summernote.ttf",
        "dist/font/summernote.woff",
        "dist/font/summernote.woff2"
      ]
    }
  ]
}